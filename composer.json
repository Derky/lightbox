{
	"name": "vse/lightbox",
	"type": "phpbb-extension",
	"description": "Lightbox for phpBB will resize posted images to a maximum set width and display them fullscreen in an elegant Lightbox overlay effect.",
	"homepage": "https://github.com/VSEphpbb/lightbox",
	"version": "1.0.5",
	"keywords": ["phpbb", "extension", "lightbox", "image", "resizer"],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Matt Friedman",
			"homepage": "http://vsephpbb.github.io",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "Lightbox",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/lightbox",
			"filename": "version_check"
		}
	}
}
